{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"outDir": "out",
		"lib": [
			"es6"
		],
		"sourceMap": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"noFallthroughCasesInSwitch": true,
		"allowUnreachableCode": false,
		"noImplicitAny": false,
		"noImplicitReturns": true,
		"alwaysStrict": true,
		"noImplicitThis": true,
		"strict": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true
	},
	"exclude": [
		"node_modules",
		".vscode-test"
	]
}
