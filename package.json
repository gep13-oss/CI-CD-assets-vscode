{
	"name": "ci-cd-assets-vscode",
	"displayName": "CI/CD Assets",
	"publisher": "gep13",
	"description": "Set of commands for downloading CI/CD assets.",
	"version": "0.1.0",
	"icon": "images/cicd.png",
	"private": true,
	"author": {
		"name": "gep13"
	},
	"bugs": {
		"url": "https://github.com/gep13/CI-CD-assets-vscode/issues"
	},
	"homepage": "https://github.com/gep13/CI-CD-assets-vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/gep13/CI-CD-assets-vscode"
	},
	"main": "./out/extension",
	"galleryBanner": {
		"color": "#DCEBFC",
		"theme": "light"
	},
	"activationEvents": [
		"onCommand:chocolatey.new",
	],
	"engines": {
		"vscode": "^1.24.0"
	},
	"categories": [
		"Other"
	],
	"contributes": {
		"configuration": [
			{
			}
		],
		"commands": [
			{
				"command": "chocolatey.new",
				"title": "Chocolatey: Create new Chocolatey package"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "%APPDATA%/npm/tsc -p ./",
		"watch": "%APPDATA%/npm/tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/byline": "^4.2.31",
		"@types/expect": "^1.20.3",
		"@types/ini": "^1.3.29",
		"@types/mocha": "^2.2.48",
		"@types/node": "^6.0.113",
		"@types/node-fetch": "^1.6.9",
		"@types/xml2js": "^0.4.3",
		"expect": "^21.2.1",
		"mocha": "^5.2.0",
		"tslint": "^5.8.0",
		"typemoq": "^2.1.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.6"
	}
}
